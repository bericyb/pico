<table>
    <thead>
        <tr>
            {{#each columns}}
            <th>{{name}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            {{#each ../columns}}
            <td>
                {{#if accessor}}
                    {{lookup ../this accessor}}
                {{else}}
                    {{../this}}
                {{/if}}
            </td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>